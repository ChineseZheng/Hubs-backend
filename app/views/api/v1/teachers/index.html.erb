<p id="notice"><%= notice %></p>

<h1>Api V1 Teachers</h1>

<table>
  <thead>
    <tr>
      <th>Avatar</th>
      <th>Name</th>
      <th>Account</th>
      <th>Password</th>
      <th>Email</th>
      <th>Description</th>
      <th>School</th>
      <th>Student id list</th>
      <th>Course id list</th>
      <th>Upload course id list</th>
      <th>Loading info</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_teachers.each do |api_v1_teacher| %>
      <tr>
        <td><%= api_v1_teacher.avatar %></td>
        <td><%= api_v1_teacher.name %></td>
        <td><%= api_v1_teacher.account %></td>
        <td><%= api_v1_teacher.password %></td>
        <td><%= api_v1_teacher.email %></td>
        <td><%= api_v1_teacher.description %></td>
        <td><%= api_v1_teacher.school_id %></td>
        <td><%= api_v1_teacher.student_id_list %></td>
        <td><%= api_v1_teacher.course_id_list %></td>
        <td><%= api_v1_teacher.upload_course_id_list %></td>
        <td><%= api_v1_teacher.loading_info %></td>
        <td><%= link_to 'Show', api_v1_teacher %></td>
        <td><%= link_to 'Edit', edit_api_v1_teacher_path(api_v1_teacher) %></td>
        <td><%= link_to 'Destroy', api_v1_teacher, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api V1 Teacher', new_api_v1_teacher_path %>
