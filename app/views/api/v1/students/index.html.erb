<p id="notice"><%= notice %></p>

<h1>Api V1 Students</h1>

<table>
  <thead>
    <tr>
      <th>Avatar</th>
      <th>Name</th>
      <th>Account</th>
      <th>Password</th>
      <th>Email</th>
      <th>Description</th>
      <th>Course id list</th>
      <th>Course progress</th>
      <th>Loading info</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_students.each do |api_v1_student| %>
      <tr>
        <td><%= api_v1_student.avatar %></td>
        <td><%= api_v1_student.name %></td>
        <td><%= api_v1_student.account %></td>
        <td><%= api_v1_student.password %></td>
        <td><%= api_v1_student.email %></td>
        <td><%= api_v1_student.description %></td>
        <td><%= api_v1_student.course_id_list %></td>
        <td><%= api_v1_student.course_progress %></td>
        <td><%= api_v1_student.loading_info %></td>
        <td><%= link_to 'Show', api_v1_student %></td>
        <td><%= link_to 'Edit', edit_api_v1_student_path(api_v1_student) %></td>
        <td><%= link_to 'Destroy', api_v1_student, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api V1 Student', new_api_v1_student_path %>
