<p id="notice"><%= notice %></p>

<h1>Api V1 Admins</h1>

<table>
  <thead>
    <tr>
      <th>Account</th>
      <th>Name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Password</th>
      <th>Admin type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_admins.each do |api_v1_admin| %>
      <tr>
        <td><%= api_v1_admin.account %></td>
        <td><%= api_v1_admin.name %></td>
        <td><%= api_v1_admin.email %></td>
        <td><%= api_v1_admin.phone %></td>
        <td><%= api_v1_admin.password %></td>
        <td><%= api_v1_admin.admin_type_id %></td>
        <td><%= link_to 'Show', api_v1_admin %></td>
        <td><%= link_to 'Edit', edit_api_v1_admin_path(api_v1_admin) %></td>
        <td><%= link_to 'Destroy', api_v1_admin, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api V1 Admin', new_api_v1_admin_path %>
