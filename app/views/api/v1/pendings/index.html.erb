<p id="notice"><%= notice %></p>

<h1>Api V1 Pendings</h1>

<table>
  <thead>
    <tr>
      <th>Avatar url</th>
      <th>Account</th>
      <th>Name</th>
      <th>Password</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Description</th>
      <th>User type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_pendings.each do |api_v1_pending| %>
      <tr>
        <td><%= api_v1_pending.avatar_url %></td>
        <td><%= api_v1_pending.account %></td>
        <td><%= api_v1_pending.name %></td>
        <td><%= api_v1_pending.password %></td>
        <td><%= api_v1_pending.email %></td>
        <td><%= api_v1_pending.phone %></td>
        <td><%= api_v1_pending.description %></td>
        <td><%= api_v1_pending.user_type_id %></td>
        <td><%= link_to 'Show', api_v1_pending %></td>
        <td><%= link_to 'Edit', edit_api_v1_pending_path(api_v1_pending) %></td>
        <td><%= link_to 'Destroy', api_v1_pending, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api V1 Pending', new_api_v1_pending_path %>
