<p id="notice"><%= notice %></p>

<h1>Api V1 Users</h1>

<table>
  <thead>
    <tr>
      <th>Avatar url</th>
      <th>Account</th>
      <th>Name</th>
      <th>Password</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Description</th>
      <th>User type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_users.each do |api_v1_user| %>
      <tr>
        <td><%= api_v1_user.avatar_url %></td>
        <td><%= api_v1_user.account %></td>
        <td><%= api_v1_user.name %></td>
        <td><%= api_v1_user.password %></td>
        <td><%= api_v1_user.email %></td>
        <td><%= api_v1_user.phone %></td>
        <td><%= api_v1_user.description %></td>
        <td><%= api_v1_user.user_type_id %></td>
        <td><%= link_to 'Show', api_v1_user %></td>
        <td><%= link_to 'Edit', edit_api_v1_user_path(api_v1_user) %></td>
        <td><%= link_to 'Destroy', api_v1_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api V1 User', new_api_v1_user_path %>
