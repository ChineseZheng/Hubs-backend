<p id="notice"><%= notice %></p>

<h1>Api V1 Parents</h1>

<table>
  <thead>
    <tr>
      <th>Avatar</th>
      <th>Name</th>
      <th>Account</th>
      <th>Password</th>
      <th>Email</th>
      <th>Description</th>
      <th>Course id list</th>
      <th>Children id list</th>
      <th>Loading info</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_parents.each do |api_v1_parent| %>
      <tr>
        <td><%= api_v1_parent.avatar %></td>
        <td><%= api_v1_parent.name %></td>
        <td><%= api_v1_parent.account %></td>
        <td><%= api_v1_parent.password %></td>
        <td><%= api_v1_parent.email %></td>
        <td><%= api_v1_parent.description %></td>
        <td><%= api_v1_parent.course_id_list %></td>
        <td><%= api_v1_parent.children_id_list %></td>
        <td><%= api_v1_parent.loading_info %></td>
        <td><%= link_to 'Show', api_v1_parent %></td>
        <td><%= link_to 'Edit', edit_api_v1_parent_path(api_v1_parent) %></td>
        <td><%= link_to 'Destroy', api_v1_parent, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api V1 Parent', new_api_v1_parent_path %>
